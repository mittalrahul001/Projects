<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="483.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddVendorController">
   <top>
      <AnchorPane prefHeight="466.0" prefWidth="455.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane layoutX="35.0" layoutY="56.0" prefHeight="421.0" prefWidth="473.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Vendor Name" />
                  <Label text="Address" GridPane.rowIndex="1" />
                  <Label text="Email ID" GridPane.rowIndex="2" />
                  <Label text="Contact Number" GridPane.rowIndex="3" />
                  <TextField fx:id="vendor_name" GridPane.columnIndex="1" />
                  <TextField fx:id="vendor_address" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="vendor_contact" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="vendor_email" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Button fx:id="submit" mnemonicParsing="false" onAction="#submitOnClick" text="Submit" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
            <Label layoutX="216.0" layoutY="14.0" text="Add Vendor" textFill="#ab2323">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </top>
</BorderPane>
